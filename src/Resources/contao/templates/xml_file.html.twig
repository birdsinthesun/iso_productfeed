<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:g="http://base.google.com/ns/1.0" xmlns:atom="http://www.w3.org/2005/Atom">

    <channel>
        <!-- Metadata Section -->
       
    {% for key, value in meta %}
        <{{ key }}>{{ value }}</{{ key }}>
    {% endfor %}
        

        <!-- Products Section -->
       
            {% for product in products %}
               <item>
                    {% for key, value in product %}
                        <{{ key }}>{{ value }}</{{ key }}>
                    {% endfor %}
               </item>
            {% endfor %}
        
    </channel>
</rss>