{
    "name": "birdsinthesun/iso_productfeed",
    "description": "XML-File with Productinformation of all Products",
    "keywords": [
        "contao",
        "isotope",
        "ecommerce",
        "feed",
        "productfeed",
        "xml"
    ],
    "type": "contao-module",
    "license": "commercially",
    "version":"dev-main",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Monique Hahnefeld",
            "email": "hello@bits-design.de",
            "homepage": "https://bits-desdign.de",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/birdsinthesun/iso_productfeed",
    "repositories": {
        "birdsinthesun/iso_productfeed": {
            "type": "path",
            "url": "/vendor/birdsinthesun/iso_productfeed"
        }
    },
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "contao/core-bundle": "4.13.*",
        "isotope/core": "^2.9"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bits\\IsoProductfeed\\": "src/"
        },
        "classmap": [
            "contao/"
        ],
        "exclude-from-classmap": [
            "contao/config/",
            "contao/dca/",
            "contao/languages/",
            "contao/templates/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
         
     "contao-manager-plugin": "Bits\\IsoProductfeed\\ContaoManager\\Plugin",
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
